<!DOCTYPE html>
<html lang="en" class="dark-style layout-footer-fixed" dir="ltr" data-theme="theme-default" data-template="vertical-menu-template">
<head>
    <meta charset="utf-8" />
    <meta name="viewport"
          content="width=device-width, initial-scale=1.0, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0" />
    <title>DMS - World </title>
    <link rel="icon" type="image/x-icon" href="~/favicon.ico" />
    <meta name="description" content="" />
    <!-- Fonts -->
    <link rel="stylesheet" href="~/lib/Design/css2.css" asp-append-version="true" />
    <!-- Icons -->
    <link rel="stylesheet" href="~/lib/Design/boxicons.css" asp-append-version="true" />
    <!-- Core CSS -->
    <link rel="stylesheet" href="~/lib/Design/core.css" class="template-customizer-core-css" />
    <link rel="stylesheet" href="~/lib/Design/theme-default.css" class="template-customizer-theme-css" />
    <link rel="stylesheet" href="~/lib/Design/demo.css" asp-append-version="true" />
    <!-- Core CSS -->
    <link rel="stylesheet" href="~/lib/Design/perfect-scrollbar.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/auth.css" asp-append-version="true" />
    <!-- Site CSS -->
    <script src="~/lib/design/helpers.js" asp-append-version="true"></script>
    <script src="~/lib/design/config.js" asp-append-version="true"></script>
</head>
<body>
    <div class="container-xxl">
        <div class="authentication-wrapper authentication-basic container-p-y">
            <div class="authentication-inner">
                <!-- Register -->
                <div class="card">
                    <div class="card-body">
                        @RenderBody()
                    </div>
                </div>
                <!-- /Register -->
            </div>
        </div>
    </div>
    <script src="~/lib/jquery/dist/jquery.min.js" asp-append-version="true"></script>
    <script src="~/lib/design/popper.js" asp-append-version="true"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js" asp-append-version="true"></script>
    <script src="~/lib/design/perfect-scrollbar.js" asp-append-version="true"></script>
    <script src="~/lib/design/menu.js" asp-append-version="true"></script>
    <script src="~/lib/design/main.js" asp-append-version="true"></script>
    <script src="~/js/auth.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
